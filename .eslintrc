{
  "extends": "airbnb/base",
  "globals": {
    "_gaq": true,
    "_pa": true,
    "$": true,
    "app": true,
    "alert": true,
    "canvg": true,
    "console": true,
    "escape": true,
    "WAGNER": true,
    "module": true,
    "require": true,
    "System": true,
    "user": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-new": 0,
    "no-new-func": 0,
    "func-names": 0,
    "no-console": 0,
    "padded-blocks": 0,
    "id-length": 0,
    "semi": 0,
    "quotes": [2, "single"],
    "indent": [2, 4],
    "comma-dangle": 0,
    "no-unused-expressions": 0,
    "camelcase": 0,
    "space-after-keywords": 0,
  }
}
